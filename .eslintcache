n  })\n  \n  return (\n    <Segment clearing>\n      <Formik\n        initialValues={initialValue}\n        onSubmit={handleFormSubmit}\n        validationSchema={validationSchema}\n      >\n        {({isSubmitting, dirty, isValid}) => {\n          return (\n            <Form className={'ui form'}>\n              <Header sub color={'teal'} content={'Event Details'}/>\n              <MyTextInput name={'title'} placeholder={'Event Title'}/>\n              <MySelectInput name={'category'} placeholder={'Event Category'} options={categoryData}/>\n              <MyTextArea name={'description'} placeholder={'Event description'} rows={3}/>\n              \n              <Header sub color={'teal'} content={'Event Location Details'}/>\n              <MyTextInput name={'city'} placeholder={'Event city'}/>\n              <MyTextInput name={'venue'} placeholder={'Event venue'}/>\n              <MyDateInput\n                name={'date'}\n                placeholderText={'Event Date'}\n                timeFormat={'HH:mm'}\n                showTimeSelect={true}\n                timeCaption={'time'}\n                dateFormat={'MMMM d, yyyy h:mm a'}\n              />\n              \n              <Button loading={isSubmitting}\n                      disabled={!isValid || !dirty || isSubmitting}\n                      type={'submit'}\n                      floated={'right'}\n                      positive={true}\n                      content={'submit'}\n              />\n              <Button\n                disabled={isSubmitting}\n                as={Link}\n                to={'/events'}\n                type={'submit'}\n                floated={'right'}\n                content={'cancel'}\n              />\n            </Form>\n          )\n        }\n        }\n      </Formik>\n    </Segment>\n  )\n}\n\nexport default EventForm",["126","127"],"/Users/mac/Documents/react/react-meet-up/src/common/form/MyTextInput.js",["128"],"/Users/mac/Documents/react/react-meet-up/src/common