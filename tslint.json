{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false, //Disable for typescript
    "object-literal-sort-keys": false,
    // "ordered-imports": false
    "member-access": false
  },
  "jsRules": {
    "no-console": false //Disable for javascript
  }
}